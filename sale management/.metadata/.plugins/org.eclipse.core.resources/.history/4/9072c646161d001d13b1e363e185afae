DROP DATABASE IF EXISTS [saleManagementDb]
CREATE DATABASE [saleManagementDb]
GO

USE [saleManagementDb]
GO

--
--
--CREATE TABLE
--
--

CREATE TABLE product_name
(
id INT IDENTITY,
[name] NVARCHAR(200),
created_at DATETIME,
updated_at DATETIME,
deleted_at DATETIME,
CONSTRAINT PK_product_name PRIMARY KEY (id)
)
GO

CREATE TABLE category
(
id INT IDENTITY,
[name] NVARCHAR(200),
created_at DATETIME,
updated_at DATETIME,
deleted_at DATETIME,
CONSTRAINT PK_category PRIMARY KEY (id)
)
GO

CREATE TABLE product
(
id INT IDENTITY,
category_id INT,
product_name_id INT,
quantity INT,
quantity_sold INT,
is_available BIT,
buy_price INT,
price INT,
information NVARCHAR(1000),
created_at DATETIME,
updated_at DATETIME,
deleted_at DATETIME,
CONSTRAINT PK_product PRIMARY KEY (id),
CONSTRAINT FK_product_product_name FOREIGN KEY (product_name_id) REFERENCES product_name(id),
CONSTRAINT FK_product_category FOREIGN KEY (category_id) REFERENCES category(id)
)

CREATE TABLE staff
(
id INT IDENTITY,
[name] NVARCHAR(200),
[level] INT,
[password] NVARCHAR(200),
created_at DATETIME,
updated_at DATETIME,
deleted_at DATETIME,
CONSTRAINT PK_staff PRIMARY KEY (id)
)
GO

CREATE TABLE [order]
(
id INT IDENTITY,
staff_id INT,
order_number INT,
created_at DATETIME,
updated_at DATETIME,
CONSTRAINT PK_order PRIMARY KEY (id),
CONSTRAINT FK_order_staff FOREIGN KEY (staff_id) REFERENCES staff(id)
)
GO

CREATE TABLE order_detail
(
id INT IDENTITY,
order_id INT,
product_id INT,
quantity INT,
created_at DATETIME,
updated_at DATETIME,
CONSTRAINT PK_order_detail PRIMARY KEY (id),
CONSTRAINT FK_order_detail_order FOREIGN KEY (order_id) REFERENCES [order](id),
CONSTRAINT FK_order_detail_product FOREIGN KEY (product_id) REFERENCES product(id)
)
GO

CREATE TABLE properties
(
id INT IDENTITY,
[name] NVARCHAR(200),
parent_id INT,
created_at DATETIME,
updated_at DATETIME,
deleted_at DATETIME,
CONSTRAINT PK_properties PRIMARY KEY (id)
)
GO

CREATE TABLE product_properties
(
id INT IDENTITY,
product_id INT,
properties_id INT,
created_at DATETIME,
updated_at DATETIME,
deleted_at DATETIME,
CONSTRAINT PK_product_properties PRIMARY KEY (id),
CONSTRAINT FK_product_properties_product FOREIGN KEY (product_id) REFERENCES product(id),
CONSTRAINT FK_product_properties_properties FOREIGN KEY (properties_id) REFERENCES properties(id)
)
GO

CREATE TABLE [image]
(
id INT IDENTITY,
product_name_id INT,
[name] NVARCHAR(200),
created_at DATETIME,
updated_at DATETIME,
deleted_at DATETIME,
CONSTRAINT PK_image PRIMARY KEY (id),
CONSTRAINT FK_image_product_name FOREIGN KEY (product_name_id) REFERENCES product_name(id)
)
GO

--
--
--INSERT SP
--
--

CREATE PROC insertProductName
@name NVARCHAR(200)
AS
BEGIN
	INSERT INTO product_name
	([name], created_at)
	VALUES
	(@name, GETDATE())
END
GO

CREATE PROC insertProduct
@category_id INT,
@product_name_id INT,
@quantity INT,
@is_available BIT,
@buy_price INT,
@price INT,
@information NVARCHAR(1000)
AS
BEGIN
	INSERT INTO product
	(category_id, product_name_id, quantity, quantity_sold, is_available, buy_price, price, information, created_at)
	VALUES
	(@category_id, @product_name_id, @quantity, 0, @is_available, @buy_price, @price, @information, GETDATE())
END
GO

CREATE PROC insertStaff
@name NVARCHAR(200),
@level INT,
@password NVARCHAR(200)
AS
BEGIN
	INSERT INTO staff
	([name], [level], [password], created_at)
	VALUES
	(@name, @level, @password, GETDATE())
END
GO

CREATE PROC insertOrder
@staff_id INT,
@order_number INT
AS
BEGIN
	INSERT INTO [order]
	(staff_id, order_number, created_at)
	VALUES
	(@staff_id, @order_number, GETDATE())
END
GO

CREATE PROC insertOrderDetail
@order_id INT,
@product_id INT,
@quantity INT
AS
BEGIN
	INSERT INTO order_detail
	(order_id, product_id, quantity, created_at)
	VALUES
	(@order_id, @product_id, @quantity, GETDATE())
END
GO

CREATE PROC insertCategory
@name NVARCHAR(200)
AS
BEGIN
	INSERT INTO category
	([name], created_at)
	VALUES
	(@name, GETDATE())
END
GO

CREATE PROC insertProperties
@name NVARCHAR(200),
@parent_id INT
AS
BEGIN
	INSERT INTO properties
	([name], parent_id, created_at)
	VALUES
	(@name, @parent_id, GETDATE())
END
GO

CREATE PROC insertProductProperties
@product_id INT,
@properties_id INT
AS
BEGIN
	INSERT INTO product_properties
	(product_id, properties_id, created_at)
	VALUES
	(@product_id, @properties_id, GETDATE())
END
GO

CREATE PROC insertImage
@product_name_id INT,
@name NVARCHAR(200)
AS
BEGIN
	INSERT INTO [image]
	(product_name_id, [name], created_at)
	VALUES
	(@product_name_id, @name, GETDATE())
END
GO

--
--
--UPDATE SP
--
--

CREATE PROC updateProductName
@name NVARCHAR(200),
@id INT
AS
BEGIN
	UPDATE product_name
	SET [name] = @name, updated_at = GETDATE()
	WHERE id = @id
END
GO

CREATE PROC updateProduct
@category_id INT,
@product_name_id INT,
@quantity INT,
@quantity_sold INT,
@is_available BIT,
@buy_price INT,
@price INT,
@information NVARCHAR(1000),
@id INT
AS
BEGIN
	UPDATE product
	SET category_id = @category_id, product_name_id = @product_name_id, quantity = @quantity, quantity_sold = @quantity_sold, is_available = @is_available, buy_price = @buy_price, price = @price, information = @information, updated_at = GETDATE()
	WHERE id = @id
END
GO

CREATE PROC updateProductQuantitySold
@quantity_sold INT, 
@id INT
AS
BEGIN
	UPDATE product
	SET quantity_sold = @quantity_sold, updated_at = GETDATE()
	WHERE id = @id
END
GO

CREATE PROC updateStaff
@name NVARCHAR(200),
@level INT,
@password NVARCHAR(200),
@id INT
AS
BEGIN
	UPDATE staff
	SET [name] = @name, [level] = @level, [password] = @password, updated_at = GETDATE()
	WHERE id = @id
END
GO

CREATE PROC updateOrder
@order_number INT,
@id INT
AS
BEGIN
	UPDATE [order]
	SET [order_number] = @order_number, updated_at = GETDATE()
	WHERE id = @id
END
GO

CREATE PROC updateOrderDetail
@order_id INT,
@product_id INT,
@quantity INT,
@id INT
AS
BEGIN
	UPDATE order_detail
	SET order_id = @order_id, product_id = @product_id, quantity = @quantity, updated_at = GETDATE()
	WHERE id = @id
END
GO

CREATE PROC updateCategory
@name NVARCHAR(200),
@id INT
AS
BEGIN
	UPDATE category
	SET [name] = @name, updated_at = GETDATE()
	WHERE id = @id
END
GO

CREATE PROC updateProperties
@name NVARCHAR(200),
@parent_id INT,
@id INT
AS
BEGIN
	UPDATE properties
	SET [name] = @name, parent_id = @parent_id, updated_at = GETDATE()
	WHERE id = @id
END
GO

CREATE PROC updateProductProperties
@product_id INT,
@properties_id INT,
@id INT
AS
BEGIN
	UPDATE product_properties
	SET product_id = @product_id, properties_id = @properties_id, updated_at = GETDATE()
	WHERE id = @id
END
GO

CREATE PROC updateImage
@product_name_id INT,
@name NVARCHAR(200),
@id INT
AS
BEGIN
	UPDATE [image]
	SET product_name_id = @product_name_id, [name] = @name, updated_at = GETDATE()
	WHERE id = @id
END
GO

--
--
--DELETE SP
--
--

CREATE PROC deleteStatement
@tableName NVARCHAR(200),
@id INT
AS
BEGIN
	EXEC('UPDATE [' + @tableName + ']' + ' SET deleted_at = GETDATE() WHERE id = ' + @id)
END
GO

