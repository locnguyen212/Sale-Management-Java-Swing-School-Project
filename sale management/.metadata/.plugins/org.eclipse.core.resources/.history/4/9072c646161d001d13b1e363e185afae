--
--
-- PARAMETERS SP
--
--

insertProductName(name)
insertProduct(category_id , product_name_id, quantity, is_available, buy_price, price, information)
insertStaff(name, level, password)
insertOrder(staff_id, order_number)
insertOrderDetail(order_id, product_id, quantity)
insertCategory(name)
insertProperties(name, parent_id)
insertProductProperties(product_id, properties_id)
insertImage(product_name_id, name)

updateProductName(name, id)
updateProduct(category_id, product_name_id, quantity, quantity_sold, is_available, buy_price, price, information, id)
updateProductQuantitySold(quantity_sold, id)
updateStaff(name, level, password, id)
updateOrder(order_number, id)
updateOrderDetail(order_id, product_id, quantity, id)
updateCategory(name, id)
updateProperties(name, parent_id, id)
updateProductProperties(product_id, properties_id, id)
updateImage(product_name_id, name, id)

deleteStatement(tableName, id)

